{
  "name": "dt-form-renderer-playground",
  "version": "0.0.2",
  "description": "dt-form-renderer playground",
  "author": "Hayden",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DTStack/dt-form-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/DTStack/dt-form-renderer/issues"
  },
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier-write": "prettier --write '**/*.{js,jsx,tsx,ts,less}' --config ../../.prettierrc",
    "prettier": "prettier --check '**/*.{js,jsx,tsx,ts,less}' --config ../../.prettierrc",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "deploy": "npm run build && gh-pages -d dist",
    "check-types": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-layout": "^6.5.0",
    "@reduxjs/toolkit": "^1.9.3",
    "gh-pages": "^5.0.0",
    "idb": "^7.1.1",
    "monaco-editor": "0.30.1",
    "react-redux": "^8.0.5",
    "redux-logger": "^3.0.6",
    "umi": "^3.5.37"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.37",
    "lint-staged": "^10.0.7",
    "monaco-editor-webpack-plugin": "6.0.0",
    "yorkie": "^2.0.0"
  },
  "license": "ISC"
}
